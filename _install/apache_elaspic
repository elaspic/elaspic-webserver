
<VirtualHost *:80>
    ServerName elaspic.com
    ServerAlias elaspic

    WSGIDaemonProcess elaspic threads=5

    WSGIScriptAlias / /home/kimadmin/mum/index.wsgi
    WSGIProcessGroup elaspic

    Alias /static/ "/home/kimadmin/mum/mum/static/"
    Alias /favicon.ico "/home/kimadmin/mum/mum/static/favicon.ico"
    <Location "/static">
        SetHandler None
    </Location>


    <LocationMatch "\.$">
        SetHandler None
    </LocationMatch>

    <Directory /home/kimadmin/mum/> >
        WSGIProcessGroup elaspic
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>	

</VirtualHost>

